{
    "name": "srlabs/centaur",
    "description": "An opinionated implementation of Cartalyst's Sentinel for Laravel 5",
    "keywords": [
        "laravel",
        "sentinel",
        "auth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Durham",
            "email": "ryan@stagerightlabs.com",
            "homepage": "http://stagerightlabs.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Centaur\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0",
        "cartalyst/sentinel": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1",
        "orchestra/testbench": "^9.1",
        "mockery/mockery": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Centaur\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "vinkla/hashids": "A Hashids bridge for Laravel"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/oscar-grajales/sentinel.git"
        }
    ]
}
